/andrewbreytenbach/mern-book-search-engine-1/client/src/index.js",[],[],"/Users/andrewbreytenbach/mern-book-search-engine-1/client/src/App.js",[],[],"/Users/andrewbreytenbach/mern-book-search-engine-1/client/src/pages/SavedBooks.js",[],[],"/Users/andrewbreytenbach/mern-book-search-engine-1/client/src/components/Navbar.js",[],[],"/Users/andrewbreytenbach/mern-book-search-engine-1/client/src/pages/SearchBooks.js",[],[],"/Users/andrewbreytenbach/mern-book-search-engine-1/client/src/components/SignupForm.js",[],[],"/Users/andrewbreytenbach/mern-book-search-engine-1/client/src/components/LoginForm.js",[],[],"/Users/andrewbreytenbach/mern-book-search-engine-1/client/src/utils/localStorage.js",[],[],"/Users/andrewbreytenbach/mern-book-search-engine-1/client/src/utils/auth.js",["115"],[],"// use this to decode a token and get the user's information out of it\nimport decode from 'jwt-decode';\n\n// create a new class to instantiate for a user\nclass AuthService {\n  // get user data\n  getProfile() {\n    return decode(this.getToken());\n  }\n\n  // check if user's logged in\n  loggedIn() {\n    // Checks if there is a saved token and it's still valid\n    const token = this.getToken();\n    return !!token && !this.isTokenExpired(token); // handwaiving here\n  }\n\n  // check if token is expired\n  isTokenExpired(token) {\n    try {\n      const decoded = decode(token);\n      if (decoded.exp < Date.now() / 1000) {\n        return true;\n      } else return false;\n    } catch (err) {\n      return false;\n    }\n  }\n\n  getToken() {\n    // Retrieves the user token from localStorage\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    // Saves user token to localStorage\n    localStorage.setItem('id_token', idToken);\n    window.location.assign('/');\n  }\n\n  logout() {\n    // Clear user token and profile data from localStorage\n    localStorage.removeItem('id_token');\n    // this will reload the page and reset the state of the application\n    window.location.assign('/');\n  }\n}\n\nexport default new AuthService();\n","/Users/andrewbreytenbach/mern-book-search-engine-1/client/src/utils/API.js",[],[],"/Users/andrewbreytenbach/mern-book-search-engine-1/client/src/utils/mutations.js",[],[],"/Users/andrewbreytenbach/mern-book-search-engine-1/client/src/utils/queries.js",[],[],{"ruleId":"116","severity":1,"message":"117","line":49,"column":1,"nodeType":"118","endLine":49,"endColumn":34},{"ruleId":"116","severity":1,"message":"117","line":49,"column":1,"nodeType":"118","endLine":49,"endColumn":34},"import/no-anonymous-default-export","Assign instance to a variable before exporting as module default","ExportDefaultDeclaration"]